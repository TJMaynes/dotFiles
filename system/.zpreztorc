#!/bin/zsh

zstyle ':prezto:load' pmodule \
  'environment' \
  'terminal' \
  'editor' \
  'history' \
  'directory' \
  'spectrum' \
  'utility' \
  'ssh' \
  'completion' \
  'git' \
  'syntax-highlighting' \
  'history-substring-search' \
  'prompt' \
  'python' \
  'node' \
  'tmux' \
  'gpg'

zstyle ':prezto:module:ssh:load' identities 'id_rsa'

zstyle ':prezto:module:prompt' theme 'steeef'
